<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iordanis.example"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:name=".DependencyInjectedApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.iordanis.example.apichanges.MainActivityBefore"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.iordanis.example.resources.PhoneActivity" >
            <!-- Handles intents for showing a track result -->
        </activity>

        <activity android:name="com.iordanis.example.resources.TabletActivity" >
            <!-- Handles intents for showing a track result -->
        </activity>
    </application>

</manifest>